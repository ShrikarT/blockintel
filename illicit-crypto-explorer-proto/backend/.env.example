# Backend environment variables (copy to .env)

# Common
SEED_PROTECT_KEY=changeme

# FastAPI (Python) mode
PORT=8000
# If using FastAPI helpers in backend/main.py, use the following URLs (replace with real ones)
NANSEN_API_URL=
GEMINI_API_URL=

# Node/Express mode
# If running backend/server.js, use the following. PORT defaults to 5000 in Node server.
NANSEN_API_KEY=YOUR_NANSEN_KEY_HERE
NANSEN_BASE_URL=https://api.nansen.ai
GEMINI_API_KEY=YOUR_GEMINI_KEY_HERE